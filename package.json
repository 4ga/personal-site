{
  "name": "portfolio-react",
  "version": "1.0.0",
  "description": "A place where i can show some examples of my work",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --hot",
